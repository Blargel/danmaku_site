<h1><%= @user.login %></h1>
<%= link_to "Edit Account", edit_user_path(@user) if current_user==@user%>

<h2><%= @user.login %>'s Projects</h2>
<% @user.projects.first(5).each do |p| %>
  <%= link_to p.title, user_project_path(@user, p) %><br />
<% end %>
<br />
<%= link_to "See all of "+@user.login+"'s projects", user_projects_path(@user) %>
