<h1><%= @project.title %></h1>
<% if current_user && current_user.is_owner_of(@project) %>
  <%= link_to "Edit", edit_user_project_path(@user, @project) %> |
  <%= link_to "Delete", user_project_path(@user, @project), :method => :delete, :confirm => "Are you sure?" %>
<% end %>

<p><%= @project.description %></p>
